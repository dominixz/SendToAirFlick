<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Author</key>
	<string>Norio Nomura</string>
	<key>CFBundleDisplayName</key>
	<string>SendUstreamToAirFlick</string>
	<key>CFBundleIdentifier</key>
	<string>com.github.norio-nomura.sendUstreamToAirFlick</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0.1</string>
	<key>CFBundleVersion</key>
	<string>1.0.1</string>
	<key>Chrome</key>
	<dict>
		<key>Global Page</key>
		<string>global.html</string>
	</dict>
	<key>Content</key>
	<dict>
		<key>Scripts</key>
		<dict>
			<key>End</key>
			<array>
				<string>injectToContent.js</string>
			</array>
		</dict>
		<key>Whitelist</key>
		<array>
			<string>http://www.ustream.tv/channel/*</string>
			<string>http://www.ustream.tv/recorded/*</string>
		</array>
	</dict>
	<key>Description</key>
	<string>Send Ustream to AirFlick</string>
	<key>ExtensionInfoDictionaryVersion</key>
	<string>1.0</string>
	<key>Permissions</key>
	<dict>
		<key>Website Access</key>
		<dict>
			<key>Allowed Domains</key>
			<array>
				<string>www.ustream.tv</string>
			</array>
			<key>Include Secure Pages</key>
			<true/>
			<key>Level</key>
			<string>Some</string>
		</dict>
	</dict>
	<key>Update Manifest URL</key>
	<string>http://norio-nomura.github.com/SendUstreamToAirFlick/Manifest.plist</string>
	<key>Website</key>
	<string>http://norio-nomura.github.com/SendUstreamToAirFlick/</string>
</dict>
</plist>
